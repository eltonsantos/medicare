<div id="<%= dom_id medicine %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= medicine.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Quantity:</strong>
    <%= medicine.quantity %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Medicine validity:</strong>
    <%= medicine.medicine_validity %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Medicine insert:</strong>
    <%= medicine.medicine_insert %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Used to:</strong>
    <%= medicine.used_to %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Purchase date:</strong>
    <%= medicine.purchase_date %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= medicine.user_id %>
  </p>

</div>
